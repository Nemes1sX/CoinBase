@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <div id="app">
        <router-view></router-view>
    </div>
</div>
<script src="~/js/Pages/Index.vue.js"></script>

<script>
    const routes = [
        {
            path: "/",
            component: Index,
        },
    ];


    const router = new VueRouter({
        routes: routes,
        mode: "history",
        base: "/",
    });

    var app = new Vue({
        el: "#app",
        router: router,
    });
</script>

